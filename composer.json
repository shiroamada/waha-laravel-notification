{
    "name": "shiroamada/waha-laravel-notification",
    "description": "WAHA Notifications channel for Laravel 11 and Above",
    "keywords": ["laravel", "notifications", "waha"],
    "homepage": "https://github.com/shiroamada/waha-laravel-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Shiro Amada",
            "role": "Developer",
            "email": "shiro_amada08th@yahoo.com",
            "homepage": "https://github.com/shiroamada"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/notifications": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Waha\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Waha\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Waha\\WahaServiceProvider"
            ]
        }
    }
}
